<?xml version="1.0" encoding="UTF-8"?>
<plugin title="dvt-framework" loader="DEFAULT">
    <overlays>
        <overlay id="dataaccess" resourcebundle="api/messages/messages" loadatstart="false"></overlay>
    </overlays>
    <lifecycle-listener class="pt.webdetails.cpk.CpkLifecycleListener"/>
    <static-paths>
        <static-path url="/dvt-framework/static" localFolder="static"/>
        <static-path url="/dvt-framework/resources" localFolder="resources"/>
        <static-path url="/dvt-framework/seo" localFolder="static/custom/seo/"/>
    </static-paths>
    <content-types>
        <content-type>
            <title>European Agency for Safety &amp; Health at Work - Data Visualisation Tool - All Ages</title>
            <description>All Ages by Zylk.net</description>
            <company name="Zylk.net" url="" logo="@PLUGIN_COMPANY_LOGO@"></company>
            <icon-url>api/repos/dvt-framework/static/custom/img/favicon.ico</icon-url>
        </content-type>
    </content-types>
    <content-generator
        id="dvt-framework"
        title="DVT framework by Zylk.net"
        type="dvt-framework"
        class="pt.webdetails.cpk.CpkContentGenerator"/>
    <!-- Menu entry -->
    <menu-items>

        <!--<main-menu-item id="dvt-framework_main" anchor="tools-submenu" label="Older Workers Pilot" command="content/dvt-framework/" type="MENU_ITEM" how="LAST_CHILD"/>-->
    </menu-items>

    <external-resources>
        <!--<file context="dvt-framework">api/repos/dvt-framework/static/dvt-framework-require-js-cfg.js</file>-->
    </external-resources>
</plugin>
